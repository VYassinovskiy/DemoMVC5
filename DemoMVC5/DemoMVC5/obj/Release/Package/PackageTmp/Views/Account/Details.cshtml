@model DemoMVC5.Models.ViewModel.DetailsUserModel

@{
    ViewBag.Title = "Информация о пользователе";
}

<h2>Информация о пользователе</h2>

<div>
    <h4>Информация о пользователе</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ФИО
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FIO)
        </dd>

        <dt>
            Логин
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Login)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Редактировать", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Вернуться к списку пользователей", "Users")
</p>
